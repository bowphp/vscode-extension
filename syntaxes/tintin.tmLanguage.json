{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Bow Tintin",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.tintin",
				"match": "\\#b(if|while|for|loop|endloop|endif|endwidth|endfor|block|endblock|extends|include)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.tintin",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.tintin",
					"match": "\\\\."
				}
			]
		}
	},
	"scopeName": "text.tintin"
}